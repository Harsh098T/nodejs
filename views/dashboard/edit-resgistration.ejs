<link rel="stylesheet" href="../assets/css/css.css">

     <div class="container-fluid" style="text-align: center;">
        <h1>SIGNUP</h1>
        </div>
    <div class="container  body1">
        <form action="/signup" method="post">
            <div class="form-group">
                <label for="name">Name:</label>
                <input type="text"  name="name" required value="<%=editdata.name %>">
            </div>
            <div class="form-group">
                <label for="email">Email:</label>
                <input type="text"  name="email" required value="<%=editdata.email %>">
            </div>
            <div class="form-group">
                <label for="password">password:</label>
                <input type="password"  name="password" required value="<%=editdata.password %>">
            </div>
            <div class="form-group">
                <label for="conatct">conatct:</label>
                <input type="text"  name="conatct" required value="<%=editdata.contact %>">
            </div>
            <div class="form-group">
                <label for="address">address</label>
                <input type="text"  name="address" required value="<%=editdata.address %>">
            </div>
            <button type="submit">Submit</button>
        </form>
    </div>